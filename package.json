{
  "name": "parakeet-api",
  "version": "0.1.0",
  "description": "lit parakeet",
  "main": "dist/index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon --exec babel-node src/",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel src/ -s -D -d --out-dir build --ignore node_modules",
    "build": "npm run clean && npm run build-babel",
    "test": "nyc mocha --require @babel/register 'test/**test.js'",
    "lint": "eslint \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MekkyMayata/parakeet-api.git"
  },
  "author": "Okpallannuozo Nnaemeka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MekkyMayata/parakeet-api/issues"
  },
  "homepage": "https://github.com/MekkyMayata/parakeet-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pg-promise": "^10.7.3",
    "winston": "^3.3.3"
  }
}
